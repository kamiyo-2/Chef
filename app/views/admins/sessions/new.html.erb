<div class="main_form">
  <div class="sign_up_form">
  <div class="title_recipe">ログイン</div>
    <div class="sign_up_forms">
      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
        <%= render "admins/shared/error_messages", resource: resource %>
        <div class="field">
          <div class="filde_name">メールアドレス</div>
          <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "sign_up_fields", placeholder: "半角英数字" %>
        </div>
        <div class="field">
          <div class="filde_names">パスワード</div>
          <%= f.password_field :password, autocomplete: "current-password", class: "sign_up_fields", placeholder: "半角英数字 (6文字〜)" %>
        </div>
        <div class="actions_up">
          <%= f.submit "ログイン" %>
        </div>
      <% end %>
    </div>
  </div>
</div>